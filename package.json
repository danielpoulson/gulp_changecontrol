{
  "name": "changecontrol",
  "version": "1.0.0",
  "description": "Change Control Program",
  "main": "index.js",
  "scripts": {
    "compile:dev": "gulp compileDev",
    "delete:dist": "del ./dist",
    "start": "npm-run-all delete:dist compile:dev copy:* babel:server build:dev",
    "gulpProd": "gulp serveBuild",
    "babel:server": "babel server -d dist/server",
    "babel-server": "npm-run-all delete:dist compile-server",
    "dev:nodemon": "nodemon dist/server/server.js",
    "dev:serve": "gulp serveDev",
    "build:dev": "npm-run-all --parallel dev:*",
    "build:prod": "export NODE_ENV=production && npm-run-all gulpProd babel:server copy:*",
    "copy:packjson": "cpy 'package.json' dist",
    "copy:html": "cpy 'server/config/mail.html' dist/server/config/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danielpoulson/changecontrol.git"
  },
  "author": "Daniel Poulson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/danielpoulson/changecontrol/issues"
  },
  "homepage": "https://github.com/danielpoulson/changecontrol",
  "dependencies": {
    "body-parser": "^1.16.0",
    "cookie-parser": "^1.4.3",
    "crypto": "0.0.3",
    "del": "^2.2.2",
    "ejs": "^2.5.5",
    "eslint-plugin-flowtype": "^2.20.0",
    "express": "^4.14.1",
    "express-csv": "^0.6.0",
    "express-session": "^1.11.3",
    "foreman": "^2.0.0",
    "git": "^0.1.5",
    "jquery": "^3.1.0",
    "json2csv": "^3.2.0",
    "moment": "^2.12.0",
    "mongoose": "^4.4.11",
    "mongoose-to-csv": "^0.1.0",
    "morgan": "^1.8.0",
    "multer": "^1.1.0",
    "nodemailer": "^2.4.2",
    "nodemailer-mailgun-transport": "https://github.com/orliesaurus/nodemailer-mailgun-transport.git",
    "passport": "^0.3.0",
    "passport-local": "^1.0.0",
    "xoauth2": "^1.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.18.7",
    "browserify": "^11.1.0",
    "del": "^2.2.2",
    "eslint": "^3.15.0",
    "eslint-plugin-import": "^2.2.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.0",
    "gulp-connect": "^5.0.0",
    "gulp-csso": "^2.0.0",
    "gulp-eslint": "^3.0.1",
    "gulp-file-cache": "^0.0.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-nodemon": "^2.2.1",
    "gulp-open": "^2.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-uglify": "^2.0.1",
    "gulp-util": "^3.0.8",
    "npm-run-all": "^4.0.1",
